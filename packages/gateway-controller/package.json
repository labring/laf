{
  "name": "gateway-controller",
  "version": "0.8.7-alpha.3",
  "description": "laf - gateway controller",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "laf.js",
    "serverless",
    "laf",
    "devops"
  ],
  "dependencies": {
    "axios": "0.27.2",
    "body-parser": "^1.19.0",
    "dayjs": "^1.10.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "mongodb": "^4.1.3",
    "mongodb-uri": "^0.9.7",
    "nanoid": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mongodb-uri": "^0.9.1",
    "@types/node": "^16.7.10",
    "nodemon": "^2.0.12",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*",
      "http/*"
    ],
    "delay": 1000
  }
}
