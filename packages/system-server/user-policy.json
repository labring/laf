{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Effect": "Allow",
          "Action": [
              "s3:GetBucketPolicy",
              "s3:GetObject",
              "s3:ListBucket",
              "s3:ListBucketMultipartUploads",
              "s3:ListMultipartUploadParts",
              "s3:PutObject",
              "s3:DeleteObject",
              "s3:GetBucketLocation"
          ],
          "Resource": [
              "arn:aws:s3:::${aws:username}-*"
          ]
      }
  ]
}
